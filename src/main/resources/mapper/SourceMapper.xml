<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qianfeng.springboot.mapper.SourceMapper">
    <select id="getSourceList" resultType="source">
        SELECT * FROM source
    </select>
    <select id="getSource" resultType="source">
        SELECT * FROM source WHERE source_id =#{sourceId}
    </select>
</mapper>